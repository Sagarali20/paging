@model IEnumerable<InternTest.Models.Product>
@{
    ViewBag.Title = "Home Page";
    Layout = null;

}

<table class="table table-striped table-condensed" id="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Office</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Office</td>


            </tr>

        }
    </tbody>
</table>


